{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
      "Permission": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          },
          "createdBy": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "createdAt",
          "updatedAt",
          "createdBy",
          "name",
          "description"
        ],
        "additionalProperties": false,
        "description": "A seeded Permission in the system"
      },
      "Role": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          },
          "createdBy": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "superUser": {
            "type": "boolean"
          }
        },
        "required": [
          "id",
          "createdAt",
          "updatedAt",
          "createdBy",
          "name",
          "description",
          "superUser"
        ],
        "additionalProperties": false,
        "description": "A user-created Role in the system"
      },
      "RolePermission": {
        "type": "object",
        "properties": {
          "roleID": {
            "type": "number"
          },
          "permissionID": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          },
          "createdBy": {
            "type": "string"
          }
        },
        "required": [
          "roleID",
          "permissionID",
          "createdAt",
          "updatedAt",
          "createdBy"
        ],
        "additionalProperties": false,
        "description": "A relation model between Role and Permission models"
      },
      "RoleWithPermissions": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          },
          "createdBy": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "superUser": {
            "type": "boolean"
          },
          "permissions": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/Permission"
            }
          }
        },
        "required": [
          "createdAt",
          "createdBy",
          "description",
          "id",
          "name",
          "permissions",
          "superUser",
          "updatedAt"
        ],
        "additionalProperties": false,
        "description": "A Role with a list of the associated Permissions"
      },
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          },
          "familyName": {
            "type": "string"
          },
          "givenName": {
            "type": "string"
          },
          "upn": {
            "type": "string"
          },
          "lastActiveDirectoryUpdate": {
            "type": "string",
            "format": "date-time"
          },
          "officeLocation": {
            "type": "string"
          },
          "mail": {
            "type": "string"
          },
          "department": {
            "type": "string"
          },
          "displayName": {
            "type": "string"
          },
          "surname": {
            "type": "string"
          },
          "jobTitle": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "createdAt",
          "updatedAt"
        ],
        "additionalProperties": false,
        "description": "A User in the system. Created when an active directory user first accesses the application."
      },
      "UserRole": {
        "type": "object",
        "properties": {
          "roleID": {
            "type": "number"
          },
          "userID": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          },
          "createdBy": {
            "type": "string"
          }
        },
        "required": [
          "roleID",
          "userID",
          "createdAt",
          "updatedAt",
          "createdBy"
        ],
        "additionalProperties": false,
        "description": "A relational model between Role and Permission models"
      },
      "UserWithRolesAndPermissions": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          },
          "familyName": {
            "type": "string"
          },
          "givenName": {
            "type": "string"
          },
          "upn": {
            "type": "string"
          },
          "lastActiveDirectoryUpdate": {
            "type": "string",
            "format": "date-time"
          },
          "officeLocation": {
            "type": "string"
          },
          "mail": {
            "type": "string"
          },
          "department": {
            "type": "string"
          },
          "displayName": {
            "type": "string"
          },
          "surname": {
            "type": "string"
          },
          "jobTitle": {
            "type": "string"
          },
          "roles": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/RoleWithPermissions"
            }
          }
        },
        "required": [
          "createdAt",
          "id",
          "roles",
          "updatedAt"
        ],
        "additionalProperties": false,
        "description": "A User with a list of their associated Roles and Permissions"
      }
    }
  }